<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.


    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>


    <!--
         AppCompat V7兼容主题
          *******************************************************************************
      -->
    <style name="AppBaseAppCompatTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/main_red_color</item>
        <item name="colorPrimaryDark">@color/main_red_color_dark</item>
        <item name="colorAccent">@color/main_red_color</item>
        <item name="android:windowAnimationStyle">@style/Window.Animation</item>
        <item name="android:fitsSystemWindows">false</item>
        <item name="android:clipToPadding">true</item>
    </style>
    <!--  @style/Widget.AppCompat.Light.PopupMenu.Overflow
    @style/Widget.AppCompat.ActionMode
    -->
    <style name="AppAppCompatTheme" parent="AppBaseAppCompatTheme">
        <item name="android:fitsSystemWindows">false</item>
        <item name="android:clipToPadding">true</item>
        <item name="android:windowBackground">@color/register_background</item>
        <item name="popupMenuStyle">@style/PopupMenuStyle</item>
    </style>

    <style name="PopupMenuStyle" parent="@style/Widget.AppCompat.PopupMenu.Overflow">
        <item name="overlapAnchor">true</item>
        <item name="android:dropDownHorizontalOffset">-4dp</item>
        <item name="android:dropDownVerticalOffset">4dp</item>
    </style>

    <style name="AppCompatDialog" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="colorPrimary">@color/main_red_color</item>
        <item name="colorPrimaryDark">@color/main_red_color_dark</item>
        <item name="colorAccent">@color/main_red_color</item>
        <item name="buttonBarButtonStyle">@style/mybuttonBarButtonStyle</item>
    </style>

    <style name="AppCompatDialog2" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="colorPrimary">@color/main_red_color</item>
        <item name="colorPrimaryDark">@color/main_red_color_dark</item>
        <item name="colorAccent">@color/main_red_color</item>
        <item name="buttonBarButtonStyle">@style/mybuttonBarButtonStyle</item>
    </style>

    <style name="mybuttonBarButtonStyle" parent="@style/Widget.AppCompat.Button.ButtonBar.AlertDialog">
        <item name="android:textSize">16sp</item>
    </style>

    <!--  ******************************************************************************* -->


    <style name="AppTheme" parent="AppBaseTheme">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@color/register_background</item>
        <item name="android:windowAnimationStyle">@style/Window.Animation</item>
    </style>

    <style name="AppBaseTheme.Dark" parent="android:Theme">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <style name="AppTheme.Dark" parent="AppBaseTheme.Dark">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@color/register_background</item>
        <item name="android:windowAnimationStyle">@style/Window.Animation</item>
    </style>

    <style name="AppTheme.transparent" parent="AppBaseTheme">
        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>

    <style name="ThemeHolo" parent="AppTheme">
        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>

    <style name="SplashTheme" parent="AppTheme">
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowBackground">@drawable/white_drawable</item>
    </style>

    <style name="WelcomeTheme" parent="AppTheme">
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowAnimationStyle">@null</item>
    </style>

    <style name="Fullscreen" parent="AppTheme">
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>


    <style name="Progress_Dialog" parent="android:style/Theme.Dialog">
        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
    </style>

    <style name="AnimationPopupwindow">
        <item name="android:windowEnterAnimation">@anim/anim_view_appear_from_bottom</item>
        <item name="android:windowExitAnimation">@anim/anim_view_exit_from_bottom</item>
    </style>

    <style name="AnimationChoicePop">
        <item name="android:windowEnterAnimation">@anim/anim_view_appear_from_bottom</item>
        <item name="android:windowExitAnimation">@anim/anim_view_exit_from_bottom</item>
    </style>

    <style name="SortPopupWindow">
        <item name="android:windowEnterAnimation">@anim/anim_view_scale_appear_from_top</item>
        <item name="android:windowExitAnimation">@anim/anim_view_scale_exit_from_top</item>
    </style>

    <style name="SortPopupWindow2">
        <item name="android:windowEnterAnimation">@anim/anim_view_scale_appear_from_mid_top</item>
        <item name="android:windowExitAnimation">@anim/anim_view_scale_exit_from_mid_top</item>
    </style>

    <style name="HouseDetail_PopoupWindowAnimation">
        <item name="android:windowEnterAnimation">@anim/detail_view_appear_from_top</item>
        <item name="android:windowExitAnimation">@anim/detail_view_exit_from_bottom</item>
    </style>

    <style name="CommTransparent" parent="@android:style/Theme.Translucent">
        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="Transparent40bDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:backgroundDimAmount">0.4</item>
    </style>

    <style name="TransparentActivityDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:windowBackground">@android:color/background_light</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:backgroundDimAmount">0.4</item>
    </style>

    <style name="CustomDialog" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:backgroundDimAmount">0.4</item>
    </style>


    <style name="progress_style" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>

    <style name="list_cell_divider_hor">
        <item name="android:background">#dddddd</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">1px</item>
    </style>

